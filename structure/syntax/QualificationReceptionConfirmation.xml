<?xml version="1.0" encoding="UTF-8"?>
<Syntax xmlns="urn:fdc:difi.no:2017:vefa:structure:Syntax-1">

    <Term>qualification-reception-confirmation</Term>
    <Name>Qualification Reception Confirmation</Name>

    <Namespace prefix="ubl">urn:oasis:names:specification:ubl:schema:xsd:TendererQualificationResponse-2</Namespace>
    <Namespace prefix="cac">urn:oasis:names:specification:ubl:schema:xsd:CommonAggregateComponents-2"</Namespace>
    <Namespace prefix="cbc">urn:oasis:names:specification:ubl:schema:xsd:CommonBasicComponents-2</Namespace>

    <Document>
        <Term>ubl:TendererQualificationResponse</Term>

        <Element cardinality="0..1">
            <Term>cbc:UBLVersionID</Term>
            <Description>Identifies the earliest version of the UBL 2 schema for this document type that defines all of
                the elements that might be encountered in the current instance.
            </Description>
            <Value type="FIXED">2.2</Value>
        </Element>

        <Element>
            <Term>cbc:CustomizationID</Term>
            <Name>Specification identification</Name>
            <Description>An identification of the specification containing the total set of rules regarding semantic content, cardinalities and
                business rules to which the data contained in the instance document conforms.
            </Description>
        </Element>

        <Element>
            <Term>cbc:ProfileID</Term>
            <Name>Business process type identifier</Name>
            <Description>Identifies the business process context in which the transaction appears. It enables the buyer to process the
                invoice in an appropriate way.
            </Description>
        </Element>

        <Element>
            <Term>cbc:ID</Term>
            <Name>Response identifier</Name>
            <Description>A response identifier enables  referencing the document instance for various purposes including referencing
                between transactions that are part of the same process.
            </Description>
            <Value type="EXAMPLE">534</Value>
        </Element>

        <Element>
            <Term>cbc:ContractFolderID</Term>
            <Name>Reference number</Name>
            <Description>An identifier that is specified by the buyer and used as a reference number for all documents in the procurement
                process. It is also known as procurement project identifier, procurement reference number or contract
                folder identifier.
            </Description>
        </Element>

        <Element>
            <Term>cbc:IssueDate</Term>
            <Name>Response issue date</Name>
            <Description>Date when the referred document was issued.</Description>
            <Value type="EXAMPLE">2018-02-02</Value>
        </Element>

        <Element cardinality="0..1">
            <Term>cbc:IssueTime</Term>
            <Name>Response issue time</Name>
            <Description>Time when the document was issued.</Description>
            <Value type="EXAMPLE">15:30:00</Value>
        </Element>

        <Element>
            <Term>cac:SenderParty</Term>
            <Name>Sender party</Name>

            <Element cardinality="0..1">
                <Term>cbc:EndpointID</Term>
                <Name>Sender electronic address identifier</Name>
                <Description>Electronic address of the party sending the document.</Description>

                <Attribute>
                    <Term>schemeID</Term>
                    <Name>schemeID</Name> <!-- Fixed?? og henvise til hvilken kodeliste? -->
                </Attribute>
                <Value type="EXAMPLE">232</Value>
            </Element>

            <Element cardinality="0..n">
                <Term>cac:PartyIdentification</Term>
                <Name>Party identification</Name>

                <Element>
                    <Term>cbc:ID</Term>
                    <Name>Sender identifier</Name>
                    <Description>Identifier of the party sending the document.</Description>

                    <Attribute>
                        <Term>schemeID</Term>
                        <Name>schemeID</Name> <!-- Fixed?? og henvise til hvilken kodeliste? -->
                    </Attribute>
                    <Value type="EXAMPLE">011</Value>
                </Element>
            </Element>

            <Element cardinality="0..n">
                <Term>cac:PartyName</Term>
                <Name>Party name</Name>

                <Element>
                    <Term>cbc:Name</Term>
                    <Name>Sender name</Name>
                    <Description>Name of the party sending the document.</Description>
                    <Value type="EXAMPLE">Ola Nordmann</Value>
                </Element>
            </Element>

        </Element>

        <Element>
            <Term>cac:ReceiverParty</Term>
            <Name>Receiver party</Name>

            <Element cardinality="0..1">
                <Term>cbc:EndpointID</Term>
                <Name>Receiver electronic address identifier</Name>
                <Description>Electronic address of the party receiving the document..</Description>

                <Attribute>
                    <Term>schemeID</Term>
                    <Name>schemeID</Name> <!-- Fixed?? og henvise til hvilken kodeliste? -->
                </Attribute>
                <Value type="EXAMPLE">23252</Value>
            </Element>

            <Element cardinality="0..n">
                <Term>cac:PartyIdentification</Term>
                <Name>Party identification</Name>

                <Element>
                    <Term>cbc:ID</Term>
                    <Name>Receiver standard identifier</Name>
                    <Description>Identifier of the party receiving the document..</Description>

                    <Attribute>
                        <Term>schemeID</Term>
                        <Name>schemeID</Name> <!-- Fixed?? og henvise til hvilken kodeliste? -->
                    </Attribute>
                    <Value type="EXAMPLE">007</Value>
                </Element>
            </Element>

            <Element cardinality="0..n">
                <Term>cac:PartyName</Term>
                <Name>Party name</Name>

                <Element>
                    <Term>cbc:Name</Term>
                    <Name>Receiver name</Name>
                    <Description>Name of the party receiver the document.</Description>
                    <Value type="EXAMPLE">Hans Hansen</Value>
                </Element>
            </Element>
        </Element>

        <Element cardinality="0..1">
            <Term>cac:ResolutionDocumentReference</Term>
            <Name>Resolution document reference</Name>

            <Element>
                <Term>cbc:ID</Term>
                <Name>Qualification document digest</Name>
                <Description>Reference to a qualification document.</Description>
            </Element>

            <Element cardinality="0..1">
                <Term>cac:Attachment</Term>
                <Name>Attachment</Name>

                <Element cardinality="0..1">
                    <Term>cac:ExternalReference</Term>
                    <Name>External reference</Name>

                    <Element cardinality="0..1">
                        <Term>aba:DocumentHash</Term>
                        <Name>Qualification document digest</Name>
                        <Description>Digest of the document.</Description>
                    </Element>

                    <Element>
                        <Term>cbc:HashAlgorithmMethod</Term>
                        <Name>Document digest method code</Name>
                        <Description>Code that indicates the algorithm used to calculate the hash.</Description>
                    </Element>
                </Element>
            </Element>
        </Element>

        <Element cardinality="1..n">
            <Term>cac:QualificationResolution</Term>
            <Name>Qualification resolution</Name>

            <Element>
                <Term>cbc:AdmissionCode</Term>
                <Name>Admission code</Name>
                <Description>An indicator that the economic operator has been accepted into the tendering process (true) or rejected from the tendering process (false).</Description>
                <!-- Min forklaring, se på nærmere -->
            </Element>

            <Element>
                <Term>cbc:ResolutionDate</Term>
                <Name>Document reception date</Name>
                <Description>Date when the referred document was received.</Description>
            </Element>

            <Element>
                <Term>cbc:ResolutionTime</Term>
                <Name>Document reception time</Name>
                <Description>The time at which the referenced document was received by the sender of the response.</Description>
            </Element>
        </Element>

    </Document>
</Syntax>
